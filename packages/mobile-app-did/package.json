{
  "name": "mobile-did",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "postinstall": "bash scripts/postinstall.sh",
    "handle-network": "yarn workspace @portkey-wallet/constants run handle-network",
    "android": "yarn run postinstall && react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 13 Pro'",
    "ios14": "react-native run-ios --simulator='iPhone 14 Pro'",
    "ios15": "react-native run-ios --simulator='iPhone 15 Pro'",
    "devices": "xcrun simctl list devices",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-pattern \"/**/InpageBridgeWeb3.js\"",
    "android-clean": "cd android && ./gradlew clean && cd ..",
    "ios-clean": "rm -rf ios/Pods && rm -rf ios/build",
    "pod-install": "cd ios && INCLUDE_FLIPPER=true pod install && cd ..",
    "pod-install:exclude-flipper": "cd ios && INCLUDE_FLIPPER=true pod install && cd ..",
    "pod-update": "cd ios && pod install --repo-update && cd ..",
    "clean-install": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn install && yarn android-clean && yarn ios-clean && yarn pod-install",
    "clean-start": "yarn start -- --reset-cache",
    "generate-svg": "cd ./js/assets/image && node generateSvg.js && cd ../../",
    "handle-svg": "cd ./js/assets/image && node handleSvg.js && cd ../../",
    "svg": "yarn handle-svg && yarn generate-svg",
    "build:aab": "cd android && ./gradlew bundleRelease",
    "build:apk": "cd android && ./gradlew assembleRelease",
    "build:ipa": "cd ios && fastlane build_ipa",
    "push:ios": "cd ios && fastlane distribute_firebase",
    "distribute:ios": "yarn build:ipa && yarn push:ios",
    "build:apk-test1": "yarn handle-network network-test1 && yarn build:apk",
    "build:apk-test2": "yarn handle-network network-test2 && yarn build:apk"
  },
  "workspaces": {
    "nohoist": [
      "@rneui/base",
      "@rneui/themed",
