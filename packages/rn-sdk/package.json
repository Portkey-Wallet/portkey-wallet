  "scripts": {
    "start": "react-native start",
    "clean": "echo 'Cleaning dist/ directory...' && rm -rf dist/",
    "build:index": "echo 'Building index.js...' && babel index.js --config-file ./babel.npm.config.js --out-file dist/index.js --extensions \".js,.jsx,.ts,.tsx,.json\"",
    "build:src": "echo 'Building src/ directory...' && babel src --config-file ./babel.npm.config.js --out-dir dist --extensions \".js,.jsx,.ts,.tsx\"",
    "copy:assets": "echo 'Copying assets...' && cpy \"src/**/*.(json|png|svg|ttf|d.ts)\" dist",
    "transform:declaration": "echo 'Processing .d.ts files...' && node script/transform-declaration.js",
    "ts-compile": "echo 'Compiling ts code...' && tsc",
    "build": "yarn clean && yarn build:index && yarn ts-compile && yarn build:src && yarn copy:assets && yarn transform:declaration",
    "postinstall": "node ./script/postinstall.js > postinstall.log",
    "lint": "eslint . --ext .ts,.tsx  --config .eslintrc.json",
    "prepublishOnly": "yarn build",
    "build-bundle": "./script/build-android-bundle.sh",
    "check-package": "rm -rf ./dist && babel ./src --config-file ./babel.temp.config.js --out-dir dist --extensions \".js,.jsx,.ts,.tsx\""
  },
